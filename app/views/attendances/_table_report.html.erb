<table class='table'>
  <thead>
    <tr>
      <th>Full Name</th>
      <th>Clock In</th>
      <th>Clock Out</th>
      <th colspan='3'></th>
    </tr>
  </thead>

 <tbody>
      <% @attendances.each do |attendance| %>
        <tr>
          <td><%= full_name(attendance) %></td>
          <td><%= attendance.clock_in_at %></td>
          <td><%= attendance.clock_out_at %></td>
          <td><%#= link_to t('common.show'), admin_ticket_path(ticket) %></td>
          <td><%#= link_to t('common.edit'), edit_admin_ticket_path(ticket) %></td>
          <td><%#= link_to t('common.destroy'), [:admin, ticket], method: :delete, data: { confirm: t('common.confirm') } %></td>
        </tr>
      <% end %>
    </tbody>
</table>